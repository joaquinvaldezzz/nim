{
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^@/root/(.*)$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^@/registry/(.*)$",
    "^@/styles/(.*)$",
    "^@/app/(.*)$",
    "",
    "^[./]"
  ],
  "tailwindAttributes": ["classNames"],
  "tailwindFunctions": ["cva", "cn"],
  "tailwindStylesheet": "app/globals.css",
  "overrides": [
    {
      "files": ["*.mdx"],
      "options": {
        "parser": "markdown"
      }
    }
  ]
}
